# Development environment for testing with backend at localhost:6000
# This overrides the default proxy behavior

# Set API base URL to call backend directly at port 6000
VITE_API_BASE_URL=http://localhost:6000

# Base path
VITE_BASE_PATH=/

# Proxy target (not used when VITE_API_BASE_URL is set)
VITE_API_PROXY_TARGET=http://localhost:6000
